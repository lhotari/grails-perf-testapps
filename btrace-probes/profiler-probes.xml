<btrace-probes namespace="profiler-probes">

 <probe name="profiler-entry">
    <map>
        <clazz>org.springframework.boot.SpringApplication</clazz>
        <method>run</method>
    </map>
 </probe>

 <probe name="profiler-exit">
    <map>
        <clazz>org.springframework.boot.SpringApplication</clazz>
        <method>run</method>
        <location>
           <value>RETURN</value>
        </location>
    </map>
 </probe>

</btrace-probes>
